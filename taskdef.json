{
  "family": "flask-ecs-app",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "flask-ecs-app",
      "image": "160596073886.dkr.ecr.us-east-1.amazonaws.com/flask-ecs-app:<IMAGE_TAG>",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80,
          "protocol": "tcp"
        }
      ]
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::160596073886:role/aws-service-role/ecs.amazonaws.com/AWSServiceRoleForECS"
}
